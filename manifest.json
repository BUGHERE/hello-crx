{
  "manifest_version": 2,
  "name": "hello world",
  "version": "1.0",
  "description": "hello",
  "icons": {
    "128": "img/logo.png",
    "48": "img/logo.png",
    "16": "img/logo.png"
  },
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "popup.html"
  },
  "homepage_url": "https://github.com/BUGHERE",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/script.js"],
      "run_at": "document_start"
    }
  ]
}